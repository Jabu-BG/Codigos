```c

#include <stdio.h>
#include <stdlib.h>

#define TAM 10

typedef struct {
    int R, F, Q;
    int vet[TAM];
} filavet;

void enfileira(int valor, filavet *p) {
    if (p->Q == TAM) {
        printf("\nFila Cheia!");
        exit(0);
    }

    if (p->R == TAM - 1)
        p->R = 0;
    else
        p->R++;

    p->vet[p->R] = valor;
    p->Q++;
}

int desenfileira(filavet *p) {
    int aux;

    if (p->Q == 0) {
        printf("\nFila Vazia!");
        exit(1);
    }

    aux = p->vet[p->F];

    if (p->F == TAM - 1)
        p->F = 0;
    else
        p->F++;

    p->Q--;
    return aux;
}

int main() {
    filavet fila;
    int valor;

    fila.R = -1;
    fila.F = 0;
    fila.Q = 0;

    for (int i = 0; i < TAM; i++) {
        printf("\nDigite o valor a ser enfileirado: ");
        scanf("%d", &valor);
        enfileira(valor, &fila);
    }

    printf("\nElementos desenfileirados:\n");
    for (int i = 0; i < TAM; i++) {
        printf("%d ", desenfileira(&fila));
    }

    return 0;
}
